              "settings": {
                "heading": "How is the weight of a helmet determined?",
                "open_by_default": false,
                "icon": "none",
                "width": 20
              },
              "blocks": {
                "text_xyFbb9": {
                  "type": "text",
                  "name": "t:names.text",
                  "settings": {
                    "text": "<p>The listed weight is measured on a size M helmet in a solid color. Graphics and special finishes may add approximately 50 grams due to additional protective coating.</p>",
